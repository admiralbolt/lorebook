<form class="item-form">
  <h1 class="create-title session-color"> {{title}} </h1>
  <div class="form-group">
    <div class="form-input-label">Name</div>
    {{input type="text" class="item-input" placeholder="Name" value=session.name}}
  </div>
  <div class="form-group">
    <div class="form-input-label">Visibility</div>
    <ToggleButton @value={{session.visible}} />
  </div>
  <div class="form-group">
    <div class="form-input-label">Ordinal</div>
    {{input type="number" class="item-input" value=session.ordinal}}
  </div>
  <div class="form-group">
    <div class="form-input-label">Summary</div>
    {{textarea class="item-input extra-long" placeholder="Summary" value=session.summary}}
  </div>
</form>
